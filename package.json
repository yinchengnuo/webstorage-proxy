{
  "name": "webstorage-proxy",
  "version": "1.0.0",
  "description": "一个可以用操作对象的方式操作cookie，sessionstorage，localstorage，indexDB的JavaScript库。你可以使用它在客户端存储数据、文件、甚至你的代码片段，并且支持自定义方法对这些数据进行二进制加密。",
  "main": "index.js",
  "scripts": {
    "test": "npx rollup -c --watch --environment NODE_ENV:development",
    "start": "npx rollup -c --environment NODE_ENV:production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yinchengnuo/webstorage-proxy.git"
  },
  "keywords": [
    "cookie",
    "sessionStorage",
    "localStorage",
    "indexDB",
    "proxy",
    "blob"
  ],
  "author": "yinchengnuo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yinchengnuo/webstorage-proxy/issues"
  },
  "homepage": "https://github.com/yinchengnuo/webstorage-proxy#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.1.0",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-flow-entry": "^0.3.1",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-uglify": "^6.0.2"
  }
}
